<body>
<div id="nav">
	<strong class="name">DISASQA</strong>
	<button><a href="/login">Login</a></button>
	<button><a href="/signup">Signup</a></button>
</div>
<div id="back">
	<h1>DisasQA</h1>
	<h4 id="type"></h4>
</div>
</body>
<script type="text/javascript">
	var i = 0;
	var txt = 'An attempt to know some important things a little more.';
	var speed = 50;

	function typeWriter() {
  	if (i < txt.length) {
    	document.getElementById("type").innerHTML += txt.charAt(i);
    	i++;
    	setTimeout(typeWriter, speed);
  		}
	}
		i=0;
		typeWriter();
</script>
